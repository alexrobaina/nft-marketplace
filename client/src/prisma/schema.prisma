generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id               Int      @id @default(autoincrement())
  createdAt        DateTime @default(now())
  email            String   @unique
  name             String?
  coverImage       String?
  avatar           String?
  socialMediaLinks String[]
  // roles            String[] // [, "admin", "staff", ...]
}
