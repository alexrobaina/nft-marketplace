generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")

}

model Session {
  id      String   @id @default(cuid())
  userId  String
  expires DateTime
  user    User     @relation(fields: [userId], references: [id], onDelete: Cascade)
}

model User {
  id               String    @id @default(cuid())
  createdAt        DateTime  @default(now())
  email            String    @unique
  name             String?
  coverImage       String?
  image            String?
  socialMediaLinks String[]
  Session          Session[]
}
